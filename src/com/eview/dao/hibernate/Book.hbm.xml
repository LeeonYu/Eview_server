<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eview.dao.hibernate.Book" table="BookOline">
		<id name="id" column="id" type="int">
			<generator class="identity">
			</generator>
		</id>
		<property name="Bname" column="Bname" type="string"></property>
		<property name="Url" column="Url" type="string"></property>
		<property name="Addtime" column="Addtime" type="int"></property>
		<property name="Kind" column="Kind" type="int"></property>
	</class>
</hibernate-mapping>

